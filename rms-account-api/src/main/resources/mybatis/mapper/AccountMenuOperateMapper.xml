<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jzfq.rms.account.dao.AccountMenuOperateMapper">
    <resultMap id="BaseResultMap" type="com.jzfq.rms.account.bean.AccountMenuOperateKey">
        <id column="menu_no" property="menuNo" jdbcType="VARCHAR"/>
        <id column="id" property="id" jdbcType="BIGINT"/>
    </resultMap>
    <delete id="deleteByPrimaryKey" parameterType="com.jzfq.rms.account.bean.AccountMenuOperateKey">
    delete from account_menu_operate
    where menu_no = #{menuNo,jdbcType=VARCHAR}
      and id = #{id,jdbcType=BIGINT}
  </delete>
    <insert id="insert" parameterType="com.jzfq.rms.account.bean.AccountMenuOperateKey">
    insert into account_menu_operate (menu_no, id)
    values (#{menuNo,jdbcType=VARCHAR}, #{id,jdbcType=BIGINT})
  </insert>
    <insert id="insertSelective" parameterType="com.jzfq.rms.account.bean.AccountMenuOperateKey">
        insert into account_menu_operate
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="menuNo != null">
                menu_no,
            </if>
            <if test="id != null">
                id,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="menuNo != null">
                #{menuNo,jdbcType=VARCHAR},
            </if>
            <if test="id != null">
                #{id,jdbcType=BIGINT},
            </if>
        </trim>
    </insert>


    <select id="queryOldMenuOperateList" parameterType="java.lang.String" resultType="java.lang.String">


    select id from account_menu_operate where  menu_no = #{menuNo,jdbcType=VARCHAR}


  </select>
    <!-- deleteByMenuNo-->
    <delete id="deleteByMenuNo" parameterType="hashmap">
        delete from account_menu_operate
        <where>
            <if test="menuNo!=null and menuNo!=''">
                and menu_no = #{menuNo,jdbcType=VARCHAR}
            </if>
            <if test="id!=null and id!=''">
                and id = #{id}
            </if>
            <if test="ids!=null and ids!=''">
                and id in
                <foreach collection="ids" index="index" item="item" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="(menuNo==null and id=='') or (id==null and id=='')">
                and 1=2
            </if>
        </where>

    </delete>

</mapper>