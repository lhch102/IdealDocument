      <if test="p.tacticsNo != null and p.tacticsNo != ''">
        and tactics_No = #{p.tacticsNo,jdbcType=VARCHAR}
      </if>
      <if test="p.tacticsName != null and p.tacticsName != ''">
        and locate(#{p.tacticsName,jdbcType=VARCHAR}, tactics_name) > 0
      </if>
      <if test="p.tacticsState != null and p.tacticsState != ''">
        and tactics_state = #{p.tacticsState,jdbcType=INTEGER}
      </if>
      <if test="p.tacticsType != null and p.tacticsType != ''">
        and tactics_type = #{p.tacticsType,jdbcType=INTEGER}
      </if>
      <if test="p.deleteflag != null">
        and deleteflag = #{p.deleteflag,jdbcType=INTEGER}
      </if>
    </where>
    order by tactics_No asc
  </select>

